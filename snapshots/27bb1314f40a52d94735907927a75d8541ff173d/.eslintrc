{
  "parser": @typescript-eslint/parser,
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module",
    "ecmaFeatures": {}
  },
  "plugins": [],
  "extends": [
  "google",
  "plugin:@typescript-eslint/recommended",
  "plugin:prettier/recommended",
  "prettier/@typescript-eslint"
],
  "rules": {
  "require-jsdoc": [
    "off"
  ],
  "prefer-arrow-callback": [
    "error"
  ]
}
}
