{
  "name": "<%= name %>",
  "description": "<%= description %>",
  "version": "<%= version %>",
  "keywords": <%- formatArray(keywords) %>,
  "author": {
    "name": "<%= author %>",
    "email": "<%= email %>"
  },
  "contributors": <%- contributors %>,
  "license": "<%= license %>",
  "engines": {
    "node": "<%- nodeVersion %>"
  },
  "main": "<%= main %>",
  "files": <%- formatArray(files) %>,
  "scripts": {
    "test": "gulp test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= github %>/<%= module %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= github %>/<%= module %>/issues"
  },
  "homepage": "https://github.com/<%= github %>/<%= module %>#readme",
  "dependencies": <%- nodeDeps('deps') %>,
  "devDependencies": <%- nodeDeps('devDeps') %>,
  "peerDependencies": <%- nodeDeps('peerDeps') %>
}
